{
  "name": "@hope-ui-themes/baseline",
  "version": "0.0.0",
  "private": false,
  "description": "A modern mininalist Hope UI theme.",
  "keywords": [
    "solid",
    "solidjs",
    "ui",
    "library",
    "design",
    "system",
    "components",
    "tailwind"
  ],
  "homepage": "https://hope-ui.com",
  "bugs": {
    "url": "https://github.com/fabien-ml/hope-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabien-ml/hope-ui.git",
    "directory": "themes/baseline"
  },
  "license": "MIT",
  "author": "Fabien Marie-Louise <fabienml.dev@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "solid": "./dist/source/index.jsx",
      "import": "./dist/esm/index.js",
      "browser": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hope-ui/theme": "workspace:^"
  },
  "publishConfig": {
    "access": "public"
  }
}
